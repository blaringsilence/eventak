<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="397.0" prefWidth="472.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.ManageSponsorsController">
   <children>
      <VBox layoutX="-1.0" prefHeight="397.0" prefWidth="472.0">
         <children>
            <TextFlow prefHeight="40.0" prefWidth="329.0">
               <children>
                  <Label prefHeight="40.0" prefWidth="330.0" text="Manage Sponsor(s)">
                     <font>
                        <Font name="Cambria Bold Italic" size="19.0" />
                     </font>
                  </Label>
               </children>
            </TextFlow>
            <ListView fx:id="sponsorList" onMouseClicked="#sponsorSelect" prefHeight="268.0" prefWidth="432.0" />
            <HBox prefHeight="97.0" prefWidth="432.0" spacing="10.0">
               <children>
                  <Button fx:id="addSponsor" mnemonicParsing="false" onAction="#addSponsor" prefHeight="37.0" prefWidth="251.0" text="Add Sponsor">
                     <font>
                        <Font name="System Bold" size="12.0" />
                     </font>
                  </Button>
                  <Button fx:id="editSponsor" disable="true" mnemonicParsing="false" onAction="#editSponsor" prefHeight="37.0" prefWidth="119.0" text="Edit">
                     <font>
                        <Font name="System Bold Italic" size="12.0" />
                     </font>
                  </Button>
                  <Button fx:id="deleteSponsor" disable="true" mnemonicParsing="false" onAction="#deleteSponsor" prefHeight="37.0" prefWidth="119.0" text="Delete" textFill="#b52f2f">
                     <font>
                        <Font name="System Bold Italic" size="12.0" />
                     </font>
                  </Button>
               </children>
               <padding>
                  <Insets bottom="20.0" top="20.0" />
               </padding>
            </HBox>
         </children>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
